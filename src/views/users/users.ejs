<!DOCTYPE html>
<html lang="en">

<head>
    <%- include('../partials/head.ejs'); %>
</head>

<body>
    <%- include('../partials/header.ejs');  %>
    <table>
        <tr>
            <th>Username</th>
            <th>Email</th>
            <th>ID</th>
            <th>Rights</th>
        </tr>
        <% for (el of users) { %>
            <tr>
                <td>
                    <%= el.username %>
                </td>
                <td>
                    <%= el.email%>
                </td>
                <td>
                    <%= el.id%>
                </td>
                <td>
                    <%=el.rights%>
                </td>
                <td>
                    <a href="/users/info/<%=el.id%>">More info</a>
                </td>
            </tr>
        <% } %>
    </table>
    <input type="button" onclick="location.href='/users/create';" value="Create new" />
</body>

</html>